{
    "name": "contao/tcpdf-bundle",
    "type": "contao-bundle",
    "description": "Adds article to PDF support to Contao 4",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Leo Feyer",
            "homepage": "https://github.com/leofeyer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "contao/core-bundle": "^4.7",
        "symfony/config": "3.4.* || ^4.1 || ^5.0",
        "symfony/dependency-injection": "3.4.* || ^4.1 || ^5.0",
        "symfony/http-kernel": "3.4.* || ^4.1 || ^5.0",
        "symfony/yaml": "3.4.* || ^4.1 || ^5.0",
        "tecnickcom/tcpdf": "^6.2.22"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "extra": {
        "contao-manager-plugin": "Contao\\TcpdfBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Contao\\TcpdfBundle\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/contao/tcpdf-bundle/issues",
        "forum": "https://community.contao.org",
        "source": "https://github.com/contao/tcpdf-bundle",
        "docs": "https://docs.contao.org"
    }
}
